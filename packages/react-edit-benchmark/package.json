{
	"name": "@oh-my-pi/react-edit-benchmark",
	"version": "0.0.1",
	"private": true,
	"description": "Edit benchmark suite using React source code mutations",
	"type": "module",
	"main": "./index.ts",
	"bin": {
		"react-edit-benchmark": "index.ts"
	},
	"scripts": {
		"start": "bun run index.ts",
		"generate": "bun run generate.ts --react-dir /tmp/react-source --count-per-type 4",
		"check": "tsgo -p tsconfig.json --noEmit"
	},
	"dependencies": {
		"@oh-my-pi/pi-agent-core": "workspace:*",
		"@oh-my-pi/pi-coding-agent": "workspace:*",
		"@oh-my-pi/pi-utils": "workspace:*",
		"diff": "^8.0.3",
		"prettier": "^3.8.1"
	},
	"devDependencies": {
		"@types/diff": "^8.0.0",
		"bun-types": "^1.3.7"
	}
}
